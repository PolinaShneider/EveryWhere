{
  "expo": {
    "name": "reactnative-expo-firebase-boilerplate-v2",
    "slug": "reactnative-expo-firebase-boilerplate-v2",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "reactnative.expo.firebase.boilerplate",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Use the photo library to change your avatar.",
        "UIBackgroundModes": ["location", "fetch", "remote-notification"],
        "NSLocationWhenInUseUsageDescription": "We need your location to track your route while the app is in use.",
        "NSLocationAlwaysUsageDescription": "We need your location to track your route even when the app is in the background.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We need your location to track your route both when the app is in use and in the background."
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "reactnative.expo.firebase.boilerplate",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": "",
    "plugins": [
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "2f7b2f66-c9b7-4ea4-b5d8-ed72e78c88f7"
      }
    }
  }
}